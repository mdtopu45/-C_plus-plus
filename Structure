#include<iostream>
using namespace std;

struct record {
    char name[16];
    char phone[12];
    char email[11];
};

int main()
{
    struct record a;

    cout<<"Enter name:";
    cin>>a.name;
    cout<<"Enter phone:";
    cin>>a.phone;
    cout<<"Enter email:";
    cin>>a.email;

    cout<<" "<<a.name<<a.phone<<a.email;

    return 0;
}
